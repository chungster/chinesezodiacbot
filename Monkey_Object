 var Monkey = function(name, species, foodsEaten) {
 this.name = name;
 this.species = species;
 this.foodsEaten = foodsEaten;
 console.log("Monkey created");
 }

 var roy = new Monkey("roy", "chimpanzee", "bananas");

 var mike = new Monkey("mike", "baboon", "apples");

 var betty = new Monkey("betty", "macaque", "peanuts");

 Monkey.prototype.identity = function (){
 console.log("Hi, I am " + this.name + ", a " + this.species + " and just ate some " + this.foodsEaten);
 }

 Monkey.prototype.eatSomething = function (food){
 this.foodsEaten = food;
 }
 
 /*getters vs setters updates information*/

 Roy.identity();

 Betty.eatSomething("peas");

 Betty.foodsEaten; 

 /* Create an array to display all the foods eaten */
 
